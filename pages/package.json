{
  "name": "pages",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "opennextjs-cloudflare build",
    "build-and-run-dev": "npm run pages:build && npm run run-dev",
    "run-dev_old": "wrangler pages dev -c wrangler.toml -c ../ogp-data-fetcher/wrangler.toml -c ../cms-data-fetcher/wrangler.toml",
    "deploy-stg_old": "npm run pages:build && wrangler pages deploy --branch development",
    "deploy-prd_old": "npm run pages:build && wrangler pages deploy",
    "run-dev": "opennextjs-cloudflare build && wrangler dev -c wrangler.toml -c ../ogp-data-fetcher/wrangler.toml -c ../cms-data-fetcher/wrangler.toml",
    "deploy-stg": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env staging",
    "deploy-prd": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.8.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "^15.5.3",
    "react": "^19",
    "react-dom": "^19",
    "swiper": "^12.0.2",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250927.0",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "^15.5.4",
    "postcss": "^8",
    "tailwindcss": "^4.0.9",
    "typescript": "^5",
    "wrangler": "^4.40.3"
  }
}
